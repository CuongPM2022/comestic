<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/decorators">
	<excludes>
		<pattern>/api*</pattern>
	</excludes>
	
	<decorator name="login" page="admin/login.jsp">
		<pattern>/login*</pattern>
	</decorator>

	<decorator name="admin" page="admin/admin.jsp">
		<pattern>/admin*</pattern>
	</decorator>

	<decorator name="web" page="web/web.jsp">
		<pattern>/*</pattern>
	</decorator>

</decorators>